<!-- Update the artists index view to use the new nested resource
route URL helper to link to the index of all songs by that artist -->

<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
    <li><%= link_to artist.name, artist_path(artist) %></li>
    <%= link_to "Songs by #{artist.name}", artist_songs_path(artist) %>
  <% end %>
</ul>
